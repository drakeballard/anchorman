 <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Full Width Page
                    <small>Subheading</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="index.html">Home</a>
                    </li>
                    <li class="active">Full Width Page</li>
                </ol>
            </div>
        </div>
     <div class="row">
        <div class="col-sm-6 col-md-8">
            <div class="row">
            {{#each articlesbysource}}
                {{#each this.articlesList.articles}}
                 <div class="col-md-12 blog-item">                    
                         
                            <h2><a href="blog-post.html">{{this.title}}</a></h2>
                            <hr>
                            <a href="blog-post.html">
                            <img class="img-responsive img-hover" src="http://placehold.it/900x300" alt="">
                            </a>
                            <hr>
                             <ul class="list-inline">
                                <li><img src="assets/img/sample-logo.png" class="source-img"></li>
                                <li><i class="fa fa-user"></i>Author Name </li>
                                <li><i class="fa fa-calendar"></i>August 28, 2013</li>
                                <li><i class="fa fa-clock-o"></i>10:00</li>
                                <li>{{../articlesList.source}}</li>
                            </ul>
                            <hr>
                            <p>{{this.description}}</p>
                            <a class="btn btn-primary" href="#">Read More <i class="fa fa-angle-right"></i></a>
                            <hr>
                        </div>
                        {{/each}}
                    {{/each}}           

            </div>
        </div>
        <div class="col-sm-6 col-md-4">
            <div class="row">
            </div>
        </div>
    </div>
</div>











