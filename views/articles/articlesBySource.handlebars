<div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Full Width Page
                    <small>Subheading</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="index.html">Home</a>
                    </li>
                    <li class="active">Full Width Page</li>
                </ol>
            </div>
        </div>
{{#each articlesList.articles}}
        <div class="row blog-item-row">
            
            <div class="col-md-6">
                <a href="blog-post.html">
                    <img class="img-responsive img-hover" src="{{this.urlToImage}}" alt="">
                </a>
            </div>
            <div class="col-md-6">
                <h3><a href="{{this.url}}">{{this.title}}</a>
                </h3>
                <hr>
                     <ul class="list-inline">
                               {{!--  <li><img src="{{../sourceDetails.urlsToLogos.small}}" class="source-img"></li> --}}
                                <li><i class="fa fa-user"></i>{{this.author}}</li>
                                <li><i class="fa fa-calendar"></i>{{this.publishedAt}}</li>
                                {{!-- <li><i class="fa fa-clock-o"></i>10:00</li> --}}
                                
                            </ul>
                            <hr>
                
                <p>{{this.description}}</p>
                <a class="btn btn-primary" href="{{this.url}}">Read More <i class="fa fa-angle-right"></i></a>
            </div>
        </div>
{{/each}}
</div>
        