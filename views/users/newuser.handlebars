        </div>
    </div>
    <div class="row">
        <div class="col-sm-6 col-md-8">
           
                       
                    
                    <div class="row">
                      <h2>Sign Up</h2>
                        <div class="col-md-10 well">
                            <form class="create-update-form" action="/users/create" method="POST">
                                <div class="form-group">
                                    <label for="un">Username:</label>
                                    <input type="text" class="form-control" id="un" name="username">
                                </div>

                                <div class="form-group">
                                    <label for="em">Email:</label>
                                    <input type="text" class="form-control" id="em" name="email">
                                </div>

                                <div class="form-group">
                                    <label for="pass">Password:</label>
                                    <input type="password" class="form-control" id="pass" name="password">
                                </div>

                                <button type="submit" class="btn btn-default">Sign Up!</button>
                            </form>
                        </div>
                    </div>

        </div>
        <div class="col-sm-6 col-md-4">
            <div class="row">
                <div class="col-md-12">
                    {{> categoryGrid}} {{> countryGrid}} {{> languageGrid}}
                </div>
            </div>
        </div>
    </div>
</div>









